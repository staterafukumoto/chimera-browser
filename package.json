{
  "name": "chimera-browser",
  "version": "0.1.1",
  "description": "open source electron-based web browser",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "clear && electron-rebuild",
    "buildwin": "electron-packager . Chimera --overwrite --asar=true --platform=win32 --arch=x64 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds ",
    "buildwin32": "electron-packager . Chimera --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds ",
    "buildlinux": "electron-packager . Chimera --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds",
    "buildlinux32": "electron-packager . Chimera --overwrite --asar=true --platform=linux --arch=ia32 --icon=assets/icons/png/icon.png --prune=true --out=release-builds",
    "buildlinuxarm": "electron-packager . Chimera --overwrite --asar=true --platform=linux --arch=arm64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds",
    "buildlinuxarm32": "electron-packager . Chimera --overwrite --asar=true --platform=linux --arch=armv7l --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wayoutware/novus-browser.git"
  },
  "keywords": [
    "browser"
  ],
  "author": "wayoutware sp",
  "license": "MPL",
  "bugs": {
    "url": "https://github.com/wayoutware/novus-browser/issues"
  },
  "homepage": "https://github.com/wayoutware/novus-browser#readme",
  "dependencies": {
    "electron-prompt": "^1.3.0",
    "electron-rebuild": "^1.8.4",
    "electron-settings": "^3.2.0",
    "electron-winstaller": "^3.0.4",
    "fluent-reveal-effect": "^1.0.0",
    "node-gyp": "^4.0.0",
    "rebuild": "^0.1.2"
  },
  "devDependencies": {
    "electron": "9.4.0",
    "electron-packager": "^13.1.1"
  }
}
